{
    "name": "spendings-wallet",
    "version": "1.0.0",
    "author": "Lubomir Peikov",
    "description": "Spendings tracker",
    "scripts": {
        "ng": "ng",
        "prestart": "cp node_modules/sql.js/dist/sql-wasm.wasm src/assets",
        "start": "ionic serve --no-open -c --devapp --host 0.0.0.0 --disable-host-check --openssl-legacy-provider",
        "native": "ionic cordova run android --livereload",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint",
        "extract": "npm run extract:bg && npm run extract:en",
        "extract:bg": "ngx-translate-extract -i ./src -o ./src/assets/i18n/bg-BG.json --format json --sort",
        "extract:en": "ngx-translate-extract -i ./src -o ./src/assets/i18n/en-EN.json --format json --sort"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "17.3.12",
        "@angular/cdk": "17.3.10",
        "@angular/common": "17.3.12",
        "@angular/compiler": "17.3.12",
        "@angular/core": "17.3.12",
        "@angular/forms": "17.3.12",
        "@angular/material": "17.3.10",
        "@angular/platform-browser": "17.3.12",
        "@angular/platform-browser-dynamic": "17.3.12",
        "@angular/router": "17.3.12",
        "@capacitor-community/sqlite": "6.0.1",
        "@capacitor/android": "6.1.2",
        "@capacitor/app": "6.0.1",
        "@capacitor/core": "6.1.2",
        "@capacitor/haptics": "6.0.1",
        "@capacitor/keyboard": "6.0.2",
        "@capacitor/status-bar": "6.0.1",
        "@ionic/angular": "8.2.7",
        "@ngx-translate/core": "15.0.0",
        "@ngx-translate/http-loader": "8.0.0",
        "crypto": "1.0.1",
        "crypto-browserify": "3.12.0",
        "crypto-js": "4.2.0",
        "dayspan": "0.12.4",
        "ionicons": "7.4.0",
        "jeep-sqlite": "2.7.2",
        "line-awesome": "1.3.0",
        "moment": "2.30.1",
        "rxjs": "7.8.1",
        "sql.js": "1.10.3",
        "sqlite3": "5.1.7",
        "tslib": "2.3.0",
        "typeorm": "0.3.20",
        "zone.js": "0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "17.3.8",
        "@angular-eslint/builder": "17.5.2",
        "@angular-eslint/eslint-plugin": "17.5.2",
        "@angular-eslint/eslint-plugin-template": "17.5.2",
        "@angular-eslint/schematics": "17.5.2",
        "@angular-eslint/template-parser": "17.5.2",
        "@angular/cli": "17.3.8",
        "@angular/compiler-cli": "17.3.12",
        "@angular/language-service": "17.3.12",
        "@bartholomej/ngx-translate-extract": "8.0.2",
        "@capacitor/cli": "6.0.0",
        "@ionic/angular-toolkit": "11.0.1",
        "@types/jasmine": "5.1.4",
        "@types/node": "22.3.0",
        "@typescript-eslint/eslint-plugin": "6.21.0",
        "@typescript-eslint/parser": "6.21.0",
        "eslint": "8.57.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsdoc": "48.2.1",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "jasmine-core": "5.1.2",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "6.4.4",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "typescript": "5.2.2"
    }
}
